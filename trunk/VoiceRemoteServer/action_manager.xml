<?xml version="1.0" encoding="UTF-8"?>
<ActionManager>
<Pattern Expression="OPEN TERMINAL">
<Activity Type="SYSTEM">open /Applications/Utilities/Terminal.app</Activity>
</Pattern>
<Pattern Expression="OPEN CHESS">
<Activity Type="SYSTEM">open /Applications/Chess.app</Activity>
</Pattern>
<Pattern Expression="OPEN TEXT EDIT">
<Activity Type="SYSTEM">open /Applications/TextEdit.app</Activity>
</Pattern>
<Pattern Expression="OPEN BROWSER">
<Activity Type="SYSTEM">open /Applications/Safari.app</Activity>
</Pattern>
<Pattern Expression="HELLO">
<Activity Type="DIALOG">HELLO!</Activity>
</Pattern>
<Pattern Expression=".*">
<Activity Type="DIALOG">Got: $0</Activity>
</Pattern>
</ActionManager>
